class apples {
public  static void main(String[] args){
	int firstarray[][]={{8,9,10,11},{12,13,14,15}};
	int secondarray[][]={{30,31,32,33},{43},{4,5,6}};	
	
	System.out.println("This is the first array");
	display(firstarray);
	
	System.out.println("This is the second array");
	display(secondarray);
}
// write the method
public static void display(int x[][]){
// take one permater and take one mulidemintonal array
	for(int row=0;row<x.length;row++){
	// write the first loop through the row
	// another loop goes thorught the column
	// your incrementing by one
	// this is the fist argument of the row number
	    for(int column=0;column<x[row].length;column++){
		// it will through the exact length
		//++ your incrementing by one
		// this is the second argument which in the column number
			System.out.print(x[row][column]+"\t");
			                                // gives space between numbers
				// prints a row
		}
		System.out.println();
		// new line
		
		}
	}
